#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
  
int main(){
      int n;  
        scanf("%d",&n);
      for (int i=n; i>= 1; i--)  
       {  
         for (int j=n; j>i;j--)  
          {  
             printf(" ");  
           }  
         for (int k=1;k<=i;k++)  
          {  
             printf("*");  
         }  
              printf("\n");  
      }
    }
