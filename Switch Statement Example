#include <stdio.h>
int main() {
    int a=5;
    switch(a)
    {
   case 1:
            printf("JAVA");
            break;
   case 2:
           printf("PYTHON");
           break;
  case 3:
           printf("C");
           break;
  default :
           printf("INVALID");
           break;
   return 0;
    }
}
